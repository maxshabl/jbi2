<script type="text/javascript">
(function($) {
	$(document).ready(function(){
		if ($.cookie("cookies_ok") != 1) {
			$("#cookies_container").show();

			$("#cookie_ok").on('click touchstart', function () {
				$.cookie("cookies_ok", 1, { path: '/', expires : 30 });
				$("#cookies_container").slideUp();
				return false;
			});
		};
	});
})(jQuery);
</script>



